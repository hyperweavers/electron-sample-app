{
  "branches": [
    "stable",
    "+([0-9])?(.{+([0-9]),x}).x",
    {
      "name": "alpha",
      "prerelease": true
    },
    {
      "name": "beta",
      "prerelease": true
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    ["@semantic-release/changelog", {
      "changelogTitle": "# Changelog"
    }],
    ["@semantic-release/npm", {
      "npmPublish": false
    }],
    "@semantic-release/git",
    ["@semantic-release/github", {
      "assets": [
        {
          "path": "./packages/out/make/**/*.exe",
          "name": "App-${nextRelease.version}.exe"
        },
        {
          "path": "./packages/out/make/**/*.deb",
          "name": "App-${nextRelease.version}.deb"
        },
        {
          "path": "./packages/out/make/**/*.rpm",
          "name": "App-${nextRelease.version}.rpm"
        },
        {
          "path": "./packages/out/make/**/*.dmg",
          "name": "App-${nextRelease.version}.dmg"
        },
        {
          "path": "./packages/out/make/**/*.zip",
          "name": "App-${nextRelease.version}.zip"
        }
      ]
    }]
  ]
}